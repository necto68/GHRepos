{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "prettier",
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0,
    "prettier/prettier": ["error", {
      "singleQuote": true,
      "trailingComma": "all",
      "jsxBracketSameLine": false
    }],
    "no-unused-vars": ["error", {
      "varsIgnorePattern": "React"
    }],
    "arrow-parens": 0
  }
}
